generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model YtSites {
  id               String    @id(map: "Sites_pkey") @default(dbgenerated("uuid_generate_v4()")) @db.Uuid
  siteName         String?   @unique(map: "Sites_siteName_key") @db.VarChar
  ytChannelUrl     String?   @db.VarChar
  createdBy        String    @db.VarChar
  createdAt        DateTime? @default(now()) @db.Timestamptz(6)
  customCss        String?   @db.VarChar
  customHead       String?   @db.VarChar
  announcementText String?   @db.VarChar
  announcementUrl  String?   @db.VarChar
  navbarLinks      String[]  @db.VarChar
  ogImageUrl       String?   @db.VarChar
  siteDescription  String?   @db.VarChar
}
