generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model AppstoreSites {
  id                String    @id(map: "Sites_pkey") @db.Uuid
  siteName          String?   @unique(map: "Sites_siteName_key") @db.VarChar
  appStoreUrl       String?   @db.VarChar
  createdBy         String    @db.VarChar
  createdAt         DateTime? @default(now()) @db.Timestamptz(6)
  updatedAt         DateTime? @db.Timestamp(6)
  customCss         String?   @db.VarChar
  customHead        String?   @db.VarChar
  views             BigInt?   @default(0)
  downloadBtnClicks BigInt?   @default(0)
}
